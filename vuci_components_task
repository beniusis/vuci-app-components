config interface
	option name 'eth0'
	option proto 'static'
    option address '192.168.1.15'
    option netmask '255.255.255.0'
	option gateway '192.168.1.254'
	list dns '8.8.8.8'

config interface
	option name 'enp1s0'
	option proto 'static'
    option address '192.168.10.22'
    option netmask '255.255.255.0'
	option gateway '192.168.10.254'
	list dns '8.8.8.8'